{
  "entry": [
    "src/pages/compete-goods/index.js",
    "src/pages/login/login.js",
    "src/pages/bg-goods/bg.js",
    "src/pages/sale-secy/sale-secy.js"
    ],
  "disableCSSModules": false,
  "publicPath": "/",
  "theme": null,
  "outputPath": "./dist",
  "autoprefixer": null,
  "proxy": {
    "/api": {
      "target": "https://betabia.banggood.com/index.php",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
    },
    "/login": {
      "target": "http://localhost:8000/",
      "pathRewrite": { "^/login" : "/login.html" }
    },
    "/bg": {
      "target": "http://localhost:8000/",
      "pathRewrite": { "^/bg" : "/bg.html" }
    },
    "/sale-secy": {
      "target": "http://localhost:8000/",
      "pathRewrite": { "^/sale-secy" : "/sale-secy.html" }
    }
  },
  "extraBabelPlugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }]
      ]
    }
  },
  "dllPlugin": {
    "exclude": [
      "babel-runtime"
    ],
    "include": [
      "dva/router",
      "dva/saga",
      "dva/fetch"
    ]
  }
}
